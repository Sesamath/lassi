{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": [ "./classes", "README.md"]
  },
  "plugins": ["plugins/markdown"],
  "markdown": {
    "parser": "gfm",
    "hardwrap": true
  },
  "templates": {
    "applicationName": "Lassi",
    "disqus": "",
    "googleAnalytics": "",
    "openGraph": {
      "title": "",
      "type": "website",
      "image": "",
      "site_name": "",
      "url": ""
    },
    "meta": {
      "title": "",
      "description": "",
      "keyword": ""
    }

  },
  "opts": {
    "destination": "./documentation",
    "recurse": true,
    "template" : "./jsdoc/jaguarjs-jsdoc",
    "private" : false
  }
}
